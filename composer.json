{
  "name": "yuncms/yuncms-question",
  "description": "The Yii Questions and Answers module inspired by Stack Overflow",
  "keywords": [
    "yii",
    "yii-question",
    "question",
    "question answer",
    "artkost"
  ],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yuncms/yuncms-question/issues",
    "source": "https://github.com/yuncms/yuncms-question"
  },
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "yiisoft/yii2-jui": "~2.0.0",
    "xutl/yii2-select2-widget": "~1.0",
    "yuncms/yuncms-coin": "~2.0",
    "yuncms/yuncms-tag": "~2.0",
    "yuncms/yuncms-user": "~2.0",
    "yuncms/yuncms-attachment": "~2.0.0",
    "yuncms/yuncms-comment": "~2.0.0",
    "yuncms/yuncms-summernote-widget": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "yuncms\\question\\": ""
    }
  },
  "extra": {
    "yuncms": {
      "name": "question",
      "i18n": {
        "class": "yii\\i18n\\PhpMessageSource",
        "sourceLanguage": "en-US",
        "basePath": "@yuncms/question/messages"
      },
      "migrationNamespace": "yuncms\\question\\migrations",
      "backend": {
        "class": "yuncms\\question\\backend\\Module"
      },
      "frontend": {
        "class": "yuncms\\question\\frontend\\Module"
      }
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
